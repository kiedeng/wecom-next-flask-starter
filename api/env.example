# 企业微信配置
# 请复制此文件为 .env 并填入实际配置

# 企业ID (在企业微信管理后台的"我的企业"页面查看)
CORP_ID=ww1234567890abcdef

# 应用Secret (在应用管理页面查看)
CORP_SECRET=your_corp_secret_here

# 应用ID (在应用管理页面查看)
AGENT_ID=1000001

# 应用配置
DEBUG=True
HOST=0.0.0.0
PORT=5000

# 前端URL (用于OAuth回调)
FRONTEND_URL=http://localhost:3000

# 数据库配置 (如果需要)
# DATABASE_URL=sqlite:///app.db

# 其他配置
SECRET_KEY=your_secret_key_here
